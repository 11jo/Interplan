BEGIN @33
REQUIRE_PREDICATE GAME_IS ~BGT TOB IWDEE BG2EE EET~ @0009

COMPILE ~Derats_Magasin_2/Scripts~

COPY	~Derats_Magasin_2/pxagenor.sto~				~override~	SAY 0xc @35

////////// CARTE ET MARCHAND POUR BG2 //////////
ACTION_IF GAME_IS ~BGT TOB BG2EE EET~ THEN BEGIN
	COPY_EXISTING ~AR0700.are~						~override~
	PATCH_IF SOURCE_SIZE > 0x28f BEGIN
		LPF fj_are_structure
		INT_VAR
		fj_type			= 2    //travel
		fj_box_left		= 3770
		fj_box_top		= 278
		fj_box_right	= 3833
		fj_box_bottom	= 377
		fj_cursor_idx	= 30   //door
		fj_loc_x		= 3801
		fj_loc_y		= 328
		fj_vertex_0		= 3770 + (346 << 16)
		fj_vertex_1		= 3833 + (377 << 16)
		fj_vertex_2		= 3833 + (307 << 16)
		fj_vertex_3		= 3777 + (278 << 16)
		STR_VAR
		fj_structure_type	= region
		fj_name				= pxoageno // Nom de la boutique
		fj_destination_area	= pxoage // Code de la carte
		fj_destination_name	= enpxoa
		END
		LPF fj_are_structure
		INT_VAR
		fj_loc_x			= 3715
		fj_loc_y			= 407
		fj_orientation		= 2   //SW
		STR_VAR
		fj_structure_type	= entrance
		fj_name				= sopxoa
		END
		ADD_MAP_NOTE #3739 #362 ~Gray~ @33
	END
	BUT_ONLY
	COPY	~Derats_Magasin_2/Copy~					~override~
	COPY	~Derats_Magasin_2/pxoagen.cre~			~override~	SAY NAME1 @34	SAY NAME2 @34
	COMPILE ~Derats_Magasin_2/Dialog~
	EXTEND_TOP ~AR0334.BCS~   ~Derats_Magasin_2/Scripts/Append/pxo0334.baf~


////////// QUÊTE //////////

END

////////// MARCHAND POUR LES FOSSES NOIRES II //////////

ACTION_IF GAME_IS ~BG2EE EET~ THEN BEGIN
	EXTEND_TOP ~OHBHUB.BCS~   ~Derats_Magasin_2/Scripts/Append/pxo8100.baf~
END

////////// MARCHAND POUR IWD //////////
