	COPY_EXISTING	~pxagenor.sto~					~override~
	REMOVE_STORE_ITEM	~pxl2pora~ //Porte vers le magasin (pas de magasin en dur dans IWDEE)
						~pxl2laca~ //Lâché d’électricité (icône venue de IWDEE)
